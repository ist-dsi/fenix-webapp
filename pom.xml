                    <path>/</path>
                    <port>8080</port>
                    <warSourceDirectory>src/main/webapp</warSourceDirectory>
                    <uriEncoding>UTF-8</uriEncoding>

                    <systemProperties>
                       <org.apache.jasper.compiler.Parser.STRICT_QUOTE_ESCAPING>false</org.apache.jasper.compiler.Parser.STRICT_QUOTE_ESCAPING>
